package com.qa.java.exercises;

public class Human {

    int i = 4;
    int j = 5;
    int k;
    
    public static void main(String[] args) {
        
        Human h1 = new Human();
        
        int value = 10;
        
        h1.print(value);
        
        System.out.println(h1.k);
    
    }    
    
    void print(int value1) {
        
        k = i + j+ value1;
    }
        
}